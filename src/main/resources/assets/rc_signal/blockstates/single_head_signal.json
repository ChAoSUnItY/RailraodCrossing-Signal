{
  "multipart": [
    {
      "when": {
        "facing": "north"
      },
      "apply": {
        "model": "rc_signal:block/single_head_signal",
        "uvlock": true
      }
    },
    {
      "when": {
        "facing": "east"
      },
      "apply": {
        "model": "rc_signal:block/single_head_signal",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "facing": "south"
      },
      "apply": {
        "model": "rc_signal:block/single_head_signal",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "facing": "west"
      },
      "apply": {
        "model": "rc_signal:block/single_head_signal",
        "y": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "rc_signal:block/signal_side",
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "rc_signal:block/signal_side",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "rc_signal:block/signal_side",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "rc_signal:block/signal_side",
        "y": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "rc_signal:block/signal_post",
        "x": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "rc_signal:block/signal_post",
        "uvlock": true
      }
    }
  ]
}